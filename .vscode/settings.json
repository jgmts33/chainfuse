{
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "jest.jestCommandLine": "yarn node --experimental-vm-modules $(yarn bin jest)",
  "jestrunner.jestCommand": "yarn node --experimental-vm-modules $(yarn bin jest)",
  "jestrunner.enableYarnPnpSupport": true,
  "jestrunner.yarnPnpCommand": "yarn-4.0.0-rc.14.cjs",
  "jestrunner.debugOptions": {
    "program": "yarn node $(yarn bin jest)",
    "env": {
      "NODE_OPTIONS": "--experimental-vm-modules"
    }
  },
  "terminal.integrated.env.osx": {
    "NODE_NO_WARNINGS": "1"
  },
  "terminal.integrated.env.linux": {
    "NODE_NO_WARNINGS": "1"
  },
  "terminal.integrated.env.windows": {
    "NODE_NO_WARNINGS": "1"
  },
  "files.exclude": {
    "**/.cache": true,
    "**/.DS_Store": true,
    "**/.editorconfig": true,
    "**/.eslintcache": true,
    "**/.git": true,
    "**/.gitattributes": true,
    "**/.husky": true,
    "**/.pnp.cjs": true,
    "**/.pnp.loader.mjs": true,
    "**/.prettierignore": true,
    "**/.yarn": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/dist/": true,
    "**/yarn-error.log": true,
    "**/yarn.lock": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "cSpell.ignoreWords": [
    "browserslist",
    "chainfuse",
    "cloudfunctions",
    "corejs",
    "endregion",
    "entrypoint",
    "envalid",
    "envars",
    "eslintcache",
    "esmodules",
    "esnext",
    "execa",
    "firestore",
    "gcloudignore",
    "globby",
    "gsutil",
    "hedera",
    "koistya",
    "kriasoft",
    "miniflare",
    "pathinfo",
    "proxied",
    "refetch",
    "refetchable",
    "sourcemap",
    "spdx",
    "tokendrop",
    "tslib",
    "typechecking",
    "walletconnect",
    "webflow",
    "yarnpkg",
    "yarnrc"
  ]
}
