{
  "name": "chainfuse",
  "version": "0.0.0",
  "packageManager": "yarn@4.0.0-rc.14",
  "private": true,
  "type": "module",
  "workspaces": [
    "admin",
    "api",
    "app",
    "core",
    "run",
    "db",
    "hardhat",
    "marketplace",
    "site",
    "tokendrop"
  ],
  "scripts": {
    "postinstall": "husky install && zx ./scripts/postinstall.js",
    "start": "yarn node --experimental-vm-modules $(yarn bin zx) ./scripts/start.js",
    "db": "zx --experimental-vm-modules ./scripts/db.js",
    "cf": "zx ./scripts/cf.js",
    "tsc": "yarn workspaces foreach run tsc",
    "lint": "eslint --cache --report-unused-disable-directives .",
    "test": "node --experimental-vm-modules $(yarn bin jest)",
    "build": "yarn workspaces foreach run build",
    "deploy": "yarn workspaces foreach run deploy",
    "admin:start": "yarn workspace admin run start",
    "admin:build": "yarn workspace admin run build",
    "admin:preview": "yarn workspace admin run preview",
    "admin:deploy": "yarn workspace admin run build && yarn workspace admin run deploy",
    "app:start": "yarn workspace app run start",
    "app:build": "yarn workspace app run build",
    "app:preview": "yarn workspace app run preview",
    "app:deploy": "yarn workspace app run build && yarn workspace app run deploy",
    "run:start": "yarn workspace run start",
    "api:start": "yarn workspace api start",
    "api:build": "rollup -c --environment TARGET:api,BABEL_ENV:production",
    "api:deploy": "yarn api:build && zx ./scripts/api-deploy.js",
    "api:logs": "zx ./scripts/api-logs.js",
    "site:build": "rollup -c --environment TARGET:site,BABEL_ENV:production",
    "site:deploy": "yarn site:build && zx ./scripts/cf.js site publish",
    "site:cf": "zx ./scripts/cf.js site",
    "gcp:setup": "zx ./scripts/gcp-setup.js",
    "fs:import": "zx ./scripts/fs.js import",
    "fs:export": "zx ./scripts/fs.js export",
    "marketplace:start": "yarn workspace marketplace start",
    "marketplace:build": "yarn workspace marketplace build",
    "marketplace:preview": "yarn workspace marketplace preview",
    "marketplace:deploy": "yarn workspace marketplace deploy",
    "tokendrop:start": "yarn workspace tokendrop start",
    "tokendrop:build": "yarn workspace tokendrop build",
    "tokendrop:preview": "yarn workspace tokendrop preview",
    "tokendrop:deploy": "zx ./scripts/tokendrop-deploy.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@cloudflare/workers-types": "^3.14.1",
    "@emotion/babel-plugin": "^11.10.2",
    "@emotion/eslint-plugin": "^11.10.0",
    "@jest/globals": "^29.0.1",
    "@jest/types": "^29.0.1",
    "@miniflare/r2": "^2.7.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-run": "^2.1.0",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/eslint": "^8.4.6",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.7.13",
    "@types/prettier": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "babel-jest": "^29.0.1",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "chalk": "^5.0.1",
    "del": "^7.0.0",
    "envars": "^0.4.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "execa": "^6.1.0",
    "firebase-admin": "^11.0.1",
    "form-data": "^4.0.0",
    "globby": "^13.1.2",
    "got": "^12.3.1",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-environment-miniflare": "^2.7.1",
    "lodash": "^4.17.21",
    "miniflare": "^2.7.1",
    "minimist": "^1.2.6",
    "prettier": "^2.7.1",
    "pretty-bytes": "^6.0.0",
    "rollup": "^2.78.1",
    "source-map-support": "^0.5.21",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "whatwg-fetch": "^3.6.2",
    "wrangler": "^2.0.27",
    "zx": "^7.0.8"
  },
  "resolutions": {
    "chalk@npm:^5.0.1": "^4.1.2",
    "rollup": "^2.78.1",
    "ethereumjs-abi": "https://registry.npmjs.org/ethereumjs-abi/-/ethereumjs-abi-0.6.8.tgz"
  },
  "envars": {
    "cwd": "./env"
  }
}
